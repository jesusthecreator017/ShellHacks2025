{
  "name": "PathFinder",
  "description": "Ever get tired from navigating a website? Try out PathFinder",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "tabs"],
  "host_permissions": ["https://127.0.0.1", "hhtps://localhost/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "logo.png"
  }
}